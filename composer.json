{
    "name": "jmikola/geojson",
    "type": "library",
    "description": "GeoJSON implementation for PHP",
    "keywords": ["geo", "geospatial", "geojson"],
    "homepage": "https://github.com/jmikola/geojson",
    "license": "MIT",
    "authors": [
        { "name": "Jeremy Mikola", "email": "jmikola@gmail.com" },
        { "name": "Erik Sulymosi", "email": "erik.sulymosi@gmail.com" }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10|^11|^12",
        "squizlabs/php_codesniffer": "^4",
        "slevomat/coding-standard": "^8.0",
        "pestphp/pest-plugin-drift": "^4.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "GeoJson\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true
        }
    }
}
